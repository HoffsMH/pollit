extends ../layouts/layout

block title
  title admin

block content
  div(class="row")
    div(class="card-panel teal lighten-3 col m8 offset-m2", id="admin-panel")
      h5 Question
      div= poll.poll.question
      ul
         - for (var i = 0; i < poll.choices.length; ++i) {
           li= poll.choices[i] + ": "
         - }
      a(href='/') New Poll
      
